include ../mixins/all.pug

-
  attr = {};
  if (production) {
    attr['data-core-script'] = 'scripts/widgets/widget_slider.min.js';
  }
  
section.customers&attributes(attr)
  .container 
    .h2-heading 
      h2 Trusted by Thousands of Happy Customer
      p These are the stories of our customers who have joined us with great pleasure when using this crazy feature.
    .customer-slider
      .customer-item 
        .customer-top 
          .customer-info
            .customer-avatar 
              img(data-core-lazy, data-src=require('../../assets/images/Ellipse 1.png'), src="", alt="avatar")
            .customer-name 
              h4 Kim Young
              p Warsaw, Poland
          .customer-rate 
            span 4.5
            img(data-core-lazy, data-src=require('../../assets/images/ant-design_star-filled.png'), src="", alt="rate")
        .customer-bottom
          p “Wow... I am very happy to use this VPN, it turned out to be more than my expectations and so far there have been no problems. LaslesVPN always the best”.
